# Discord Bot Configuration
# Скопируйте этот файл в .env и заполните своими данными

# Основные настройки бота
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_client_id_here
GUILD_ID=your_discord_server_id_here
ADMIN_CHANNEL_ID=your_admin_channel_id_here

# Игровые роли
ROLE_ID_CS2=role_id_for_cs2_players
ROLE_ID_VALORANT=role_id_for_valorant_players
ROLE_ID_MINECRAFT=role_id_for_minecraft_players
ROLE_ID_GTA=role_id_for_gta_players

# Роли уведомлений
ROLE_ID_UPDATES=role_id_for_updates_notifications
ROLE_ID_EVENTS=role_id_for_events_notifications
ROLE_ID_NEWS=role_id_for_news_notifications
ROLE_ID_GIVEAWAYS=role_id_for_giveaway_notifications

# Роли персонала
ROLE_ID_HELPER=role_id_for_helper_position
ROLE_ID_MODERATOR=role_id_for_moderator_position
ROLE_ID_ADMIN=role_id_for_admin_position

# Дополнительные настройки
ADMIN_ROLE_ID=role_id_for_admin_notifications
